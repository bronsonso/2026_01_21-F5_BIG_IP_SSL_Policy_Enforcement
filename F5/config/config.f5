ltm rule prod/apps2 {
    partition prod
when HTTP_REQUEST {
    if { not [HTTP::cookie exists "HKEC_BROWSER"] } {
        if { [class match [HTTP::host] ends_with hkec_domain] } {
            set HKEC_BROWSER_set [class match -value [HTTP::host] ends_with 
hkec_domain]
        }
    }
    if { [TCP::local_port] == 443 } {
        HTTP::header replace "hkec_ssl" "on"
    }
    if { [HTTP::uri] starts_with "/tools/topbar2rp/access_mqlog.php" } {
        pool apps2_mqlog_http
    } elseif { [HTTP::host] == "apps2.hkedcity.net"  and [HTTP::cookie exists 
"HKEC_PREVIEW"] } {
        pool apps2_PREVIEW
    } else {
        pool apps2_http
    }
}
}
ltm rule prod/apps_http {
    partition prod
when HTTP_REQUEST {
    if { not [HTTP::cookie exists "HKEC_BROWSER"] } {
        if { [class match [HTTP::host] ends_with hkec_domain] } {
            set HKEC_BROWSER_set [class match -value [HTTP::host] ends_with 
hkec_domain]
        }
    }
    if { [TCP::local_port] == 443 } {
        HTTP::header replace "hkec_ssl" "on"
    }
    if { [HTTP::host] == "apps.hkedcity.net" } {
        if { [HTTP::uri] starts_with "/blogportal/" or [HTTP::uri] == 
"/blogportal" } {
        set IGNORE_404 1
        }
    }
    if { [class match [HTTP::uri] starts_with rproxy_names] } {
        pool rproxy_http
    } elseif { [HTTP::host] eq "eventcalendar.hkedcity.net" } {
        if { [TCP::local_port] == 443 } {
            HTTP::redirect "https://apps.hkedcity.net/mcal[HTTP::uri]"
            return
        } else {
            HTTP::redirect "http://apps.hkedcity.net/mcal[HTTP::uri]"
            return
        }
    } elseif { [HTTP::host] eq "ws.hkedcity.net" } {
        if  { [HTTP::uri] starts_with "/webservice/userInfoService/" or 
[HTTP::uri] == "/webservice/userInfoService"} {
            pool search_http
#            log local0. "test for 
https://ws.hkedcity.net/webservice/userInfoService/service.php"
        } elseif  { [HTTP::uri] starts_with "/webservice/role/" or [HTTP::uri] == 
"/webservice/role"} {
            pool search_http
        } else {
            pool ws_http
        }
    } elseif { [HTTP::uri] starts_with "/testpage_https" or [string tolower 
[HTTP::uri]] starts_with "/mcal/sch_activity/" } {
        if { [TCP::local_port] == 80 } {
            HTTP::redirect "https://[HTTP::host][HTTP::uri]"
            return
        } else {
            pool acladmin_http
        }
    } else {
        if { [HTTP::uri] starts_with "/media" } {
            #rateclass video
        }
        pool acladmin_http
    }
}
}

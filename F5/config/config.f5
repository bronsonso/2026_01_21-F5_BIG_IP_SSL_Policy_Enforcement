ltm rule uat/u_limit_rate_request {
    partition uat
# This iRule limits the number of HTTP Requests from a specified client IP address
to 200 HTTP Requests for 6000 seconds
# A Data Group u_http_rl_list will contain the IP addresses that require 
throttling
when HTTP_REQUEST {
# Check if the IP address is within the defined list of addresses to throttle
if { [class match [IP::client_addr] equals u_http_rl_list ] } {
        # Check if there is an entry for the client_addr in the table
        if { [ table lookup -notouch [IP::client_addr] ] != "" } {
        # If the value is less than 200 increment it by one
           # log local0. "Client Throttle: Value present for [IP::client_addr]"
                        if { [ table lookup -notouch [IP::client_addr] ] < 200 } {
                      #  log local0. "Client Throttle: Number of requests from 
client = [ table lookup -notouch [IP::client_addr] ]"
                        table incr -notouch [IP::client_addr] 1
                        } else {
                            log local0. "Client Throttle: Client [IP::client_addr]
has exceeded the number of allowed requests of [ table lookup -notouch 
[IP::client_addr] ]"
                            # This else statement is invoked when the table key 
value for the client IP address is more than 200. That is, the client has reached 
the 200 request limit
                            #        HTTP::respond 200 content {
                            #            <html>
                            #            <head>
                            #            <title>Information Page</title>
                            #            </head>
                            #            <body>
                            #              We are sorry, but the site has received
too many requests. Please try again later.
                            #            </body>
                            #            </html>
                            #        }
                       }
            } else {
                    # If there is no entry for the client_addr create a new table 
to track number of HTTP_REQUEST. Lifetime is set to 6000 seconds
                    log local0. "Client Throttle: Table created for 
[IP::client_addr]"
                    table set [IP::client_addr] 1 6000
                    }
} else {
    return
}
}
}

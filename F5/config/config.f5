        append HKEC_BROWSER_value "; Domain=[class match -value $HKEC_BROWSER_set 
equals hkec_rdomain]; Expires=$HKEC_BROWSER_expires"
        HTTP::header insert Set-Cookie "$HKEC_BROWSER_value"
        unset HKEC_BROWSER_set
        unset HKEC_BROWSER_value
        unset HKEC_BROWSER_expires
    }
    if { 0 and not [class match [IP::client_addr] equals bypass_ip_hkec_maint] } {
        HTTP::redirect "http://maint.hkedcity.net/index.html"
        return
    } elseif { [HTTP::status] == 404 or [HTTP::status] eq "500" } {
        if { [IP::addr [IP::client_addr] equals 10.0.0.0/22]} {
            #from internal servers, do nothing
        } elseif { [info exists isErrPageIgnore]} {
            #ignore all custom error page
        } elseif { [HTTP::status] == 404 and [info exists IGNORE_404] } {
            #ignore 404
        } elseif { [info exists is_from_mhkcity] } {
            HTTP::respond 404 content [subst $::static_error_message_mhkcity]
        } else {
            HTTP::respond 404 content [subst $::static_error_message]
        }
    } elseif { [HTTP::status] == 403 } {
        if { [IP::addr [IP::client_addr] equals 10.0.0.0/22]} {
            #from internal servers, do nothing
        } else {
            HTTP::respond 403 content [subst $::static_error_message_403]
        }
    }
}

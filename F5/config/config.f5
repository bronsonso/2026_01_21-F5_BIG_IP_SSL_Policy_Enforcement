ltm rule prod/hkec {
    partition prod
when HTTP_REQUEST {
    if { [HTTP::host] == "www.hkedcity.net" and ( [class match [HTTP::uri] equals 
redirect_to_edcity_hk_equal_names] or [class match [HTTP::uri] starts_with 
redirect_to_edcity_hk_start_names] ) } {
        if { [TCP::local_port] == 443 } {
            HTTP::respond 301 Location "https://www.edcity.hk[HTTP::uri]"
        } else {
            HTTP::respond 301 Location "http://www.edcity.hk[HTTP::uri]"
        }
    }
    if { [HTTP::host] == "www.hkedcity.net" and ( [class match [HTTP::uri] equals 
decommission_equal_names] or [class match [HTTP::uri] starts_with 
decommission_start_names] ) } {
        HTTP::respond 404 content [subst $::static_error_decommission_message]
    }
    if { not [HTTP::cookie exists "HKEC_BROWSER"] } {
        if { [class match [HTTP::host] ends_with hkec_domain] } {
            set HKEC_BROWSER_set [class match -value [HTTP::host] ends_with 
hkec_domain]
        }
    }
    if { [HTTP::host] == "www.hkedcity.net" and ([HTTP::uri] starts_with 
"/ereadscheme/" or [HTTP::uri] == "/ereadscheme") } {
        set URI [HTTP::uri]
        set redirect_url [class match -value $URI equals 
ereadscheme_redirect_readingcity]
        log local0. "eReadscheme need redirected: $URI"
        if { $redirect_url != ""} {
            HTTP::redirect $redirect_url
            log local0. "eReadscheme redirected: $URI -> $redirect_url"
            return
        } else {
            if { [HTTP::uri] starts_with "/ereadscheme/en/pkg/" or [HTTP::uri] == 
"/ereadscheme/en/pkg" } {
                HTTP::redirect "https://www.hkreadingcity.net/en/pkg"
                return
            } elseif { [HTTP::uri] starts_with "/ereadscheme/en/" or [HTTP::uri] 
== "/ereadscheme/en" } {
                HTTP::redirect "https://www.hkreadingcity.net/en/ereadscheme"
                return
            } elseif { [HTTP::uri] starts_with "/ereadscheme/zh-hant/pkg/" or 
[HTTP::uri] == "/ereadscheme/zh-hant/pkg" } {
                HTTP::redirect "https://www.hkreadingcity.net/zh-hant/pkg"
                return
            } elseif { [HTTP::uri] starts_with "/ereadscheme/zh-hant/" or 
[HTTP::uri] == "/ereadscheme/zh-hant" } {
                HTTP::redirect "https://www.hkreadingcity.net/zh-hant/ereadscheme"
                return
            } elseif { [HTTP::uri] starts_with "/ereadscheme/pkg/" or [HTTP::uri] 
== "/ereadscheme/pkg" } {
                HTTP::redirect "https://www.hkreadingcity.net/pkg"
                return
            } elseif { [HTTP::uri] starts_with 
"/ereadscheme/sites/default/api/handler.php" or [HTTP::uri] == 
"/ereadscheme/sites/default/api/handler.php" } {
                pool drupal_ereadscheme_http
            } else {
                HTTP::redirect "https://www.hkreadingcity.net/ereadscheme"
                return
            }
        }
    }
    if { 1 and [HTTP::host] == "www.hkedcity.net" } {
        if { [HTTP::uri] starts_with "/ihouse_tools/" or [HTTP::uri] == 
"/ihouse_tools" } {
            set IGNORE_404 1
        } elseif { [HTTP::uri] starts_with "/edv/" or [HTTP::uri] == "/edv" } {
            set IGNORE_404 1
        }
    }
    if { [class match [HTTP::uri] starts_with rproxy_names] } {
        pool rproxy_http
    } elseif { [class match [HTTP::uri] starts_with redirect_to_mainsite] } {
        HTTP::redirect "https://www.hkedcity.net/"
        return
    } elseif { [HTTP::host] == "www.hkreadingcity.net" } {
        set now [clock seconds]
        set start [clock scan "2022-03-19 16:00:00"]
        if { 0 and ($now >= $start and not [class match [IP::client_addr] equals 
bypass_ip_hkec_maint]) } {
            HTTP::redirect "https://maint.hkedcity.net/maint.html"
            return
        } else {
            pool drupal_hkreadingcity_http
        }
    } elseif { [HTTP::host] == "www-new.hkreadingcity.net" } {
        HTTP::redirect "https://[HTTP::host][HTTP::uri]"
        return
    } elseif { [HTTP::host] == "hkreadingcity.net" } {
        pool redirect_http
    } elseif { [HTTP::host] == "apps2.hkedcity.net"  and [HTTP::cookie exists 
